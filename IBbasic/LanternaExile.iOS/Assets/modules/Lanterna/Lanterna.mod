  "moduleJournal": [
    {
      "Name": "Andy's Message",
      "Tag": "AndyQuest",
      "Entries": [
        {
          "EntryTitle": "See Dalin",
          "EntryText": "One of the towns' runners, Andy, has given you a message.  Dalin, the Chief Judge, needs to speak with you right away.  He is located in the Civic Building.",
          "Tag": "andy1",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Dalin Visited",
          "EntryText": "You have spoken with Dalin.",
          "Tag": "andy2",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    },
    {
      "Name": "Dalin's Task",
      "Tag": "dalinQuest",
      "Entries": [
        {
          "EntryTitle": "Message to Verto",
          "EntryText": "Dalin has asked you to deliver a message to Verto (he is located in the guard barracks).  Tell him that a search and rescue party is to be formed.  Verto is to lead the team as soon as possible.",
          "Tag": "dalin1",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Message Delivered",
          "EntryText": "You delivered Dalin's message to Verto.",
          "Tag": "dalin2",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    },
    {
      "Name": "Find your Sister",
      "Tag": "findSister",
      "Entries": [
        {
          "EntryTitle": "Search for your Sister",
          "EntryText": "Your sister and her friends have been abducted and you must find them if you can.  Their last known location was at the secret falls.  You can go it alone or travel with the search and rescue team. If you wish to question Tindra, she can be found at the Sanctuary.",
          "Tag": "sister1",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Found your Sister",
          "EntryText": "You found your sister and her friends and set them free.",
          "Tag": "sister2",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    },
    {
      "Name": "Search and Rescue",
      "Tag": "SaRQuest",
      "Entries": [
        {
          "EntryTitle": "Joining the Search and Rescue Team",
          "EntryText": "Dalin has asked you to deliver a message to Verto (he is located in the guard barracks).  Tell him that a search and rescue party is to be formed.  Verto is to lead the team as soon as possible.",
          "Tag": "sar1",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Joined the Search and Rescue Team",
          "EntryText": "You have joined the Search and Rescue team.",
          "Tag": "sar2",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    },
    {
      "Name": "Verto's Task",
      "Tag": "vertoTask",
      "Entries": [
        {
          "EntryTitle": "Get Search and Rescue Items",
          "EntryText": "You have agreed to retrieve some items for Verto.  Go to the general store and present the list to Mero.",
          "Tag": "sarItems1",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Have Search and Rescue Items",
          "EntryText": "You have delivered the list to Mero and collected the items.",
          "Tag": "sarItems2",
          "EntryId": 0,
          "EndPoint": false
        },
        {
          "EntryTitle": "Delivered Search and Rescue Items",
          "EntryText": "You have delivered the supplies to Verto.",
          "Tag": "sarItems3",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    },
    {
      "Name": "Warn Charn",
      "Tag": "warnCharn",
      "Entries": [
        {
          "EntryTitle": "Girls Warn Charn",
          "EntryText": "The girls are off to warn Charn of The Exile's plans and hopefully rally support from the Wood Elves, Mountain Dwarves, and Talking Beasts.  When you are ready, head back to Charn.  Upon arriving, report to Dalin in the Civic Building.",
          "Tag": "warn1",
          "EntryId": 0,
          "EndPoint": false
        }
      ]
    }
  ],
