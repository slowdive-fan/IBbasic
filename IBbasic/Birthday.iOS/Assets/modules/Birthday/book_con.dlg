                          "ShowOnlyOnce": false,
                          "NodeIsActive": true,
                          "NodePortraitBitmap": "",
                          "NodeNpcName": "",
                          "conversationText": "Your fate has been both told and told, and neither telling counts.  There is only <gy>pattern</gy>, never <gy>truth</gy>.  A riddle for a riddle, young one.",
                          "IsExpanded": true,
                          "indentMultiplier": 5,
                          "subNodes": [
                            {
                              "idNum": 6,
                              "orderNum": 0,
                              "parentIdNum": 5,
                              "pcNode": true,
                              "linkTo": 0,
                              "ShowOnlyOnce": false,
                              "NodeIsActive": true,
                              "NodePortraitBitmap": "",
                              "NodeNpcName": "",
                              "conversationText": "I want no part of this riddle.  Just let me open the book.",
                              "IsExpanded": true,
                              "indentMultiplier": 6,
                              "subNodes": [
                                {
                                  "idNum": 7,
                                  "orderNum": 0,
                                  "parentIdNum": 6,
