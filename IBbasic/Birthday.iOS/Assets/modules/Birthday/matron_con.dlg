Expanded":true,"indentMultiplier":4,"subNodes":[{"idNum":87,"orderNum":0,"parentIdNum":86,"pcNode":false,"linkTo":0,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"What else can I do for you?","IsExpanded":true,"indentMultiplier":5,"subNodes":[{"idNum":100018,"orderNum":0,"parentIdNum":87,"pcNode":true,"linkTo":97,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"Is there anything more you can tell me about Mother Goat?","IsExpanded":true,"indentMultiplier":6,"subNodes":[],"actions":[],"conditions":[],"isLink":true},{"idNum":100019,"orderNum":1,"parentIdNum":87,"pcNode":true,"linkTo":106,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"What can you tell me of the Woods, Matron?","IsExpanded":true,"indentMultiplier":6,"subNodes":[],"actions":[],"conditions":[],"isLink":true},{"idNum":100020,"orderNum":2,"parentIdNum":87,"pcNode":true,"linkTo":110,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"Tell me more of Schafberg's goddess, the Wooden Crone.","IsExpanded":true,"indentMultiplier":6,"subNodes":[],"actions":[],"conditions":[],"isLink":true},{"idNum":100017,"orderNum":3,"parentIdNum":87,"pcNode":true,"linkTo":93,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"I seek healing and refreshment, Matron.","IsExpanded":true,"indentMultiplier":6,"subNodes":[],"actions":[],"conditions":[],"isLink":true},{"idNum":100021,"orderNum":4,"parentIdNum":87,"pcNode":true,"linkTo":114,"ShowOnlyOnce":false,"NodeIsActive":true,"NodePortraitBitmap":"","NodeNpcName":"","conversationText":"Nothing at the moment, Matron.  Farewell.","IsExpanded":true,"indentMultiplier":6,"subNodes":[],"actions":[],"conditions":[],"isLink":true}],"actions":[],"conditions":[],"isLink":false}],"actions":[],"conditions":[],"isLink":false}],"actions